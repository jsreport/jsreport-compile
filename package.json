{
  "name": "jsreport-compile",
  "version": "0.2.1",
  "description": "Compile jsreport into single executable",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/test.js --timeout 20000"
  },
  "homepage": "https://github.com/jsreport/jsreport-compile",
